@model IEnumerable<VillasBonaterra.ViewModels.AccessViewModel>

@{
    ViewBag.Title = "Graficas";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
<title>@ViewBag.Title</title>
<!-- Breadcrumbs-->
<ol class="breadcrumb">
    <li class="breadcrumb-item">
        <a href="@Url.Action("Index", "Home")">Tablero</a>
    </li>
    <li class="breadcrumb-item active">@ViewBag.Title</li>
</ol>
<hr>
<h1>@ViewBag.Title</h1>
<div class="card mb-3">
    <div class="card-header">
        <i class="fa fa-area-chart"></i> Ingreso de Cuotas
    </div>
    <div class="card-body">
        <canvas id="myAreaChart" width="100%" height="30"></canvas>
    </div>
    <div class="card-footer small text-muted">Ultima Actualizacion 11:59 PM</div>
</div>
<div class="row">
    <div class="col-lg-8">
        <!-- Example Bar Chart Card-->
        <div class="card mb-3">
            <div class="card-header">
                <i class="fa fa-bar-chart"></i> Gastos Fijos
            </div>
            <div class="card-body">
                <canvas id="myBarChart" width="100" height="50"></canvas>
            </div>
            <div class="card-footer small text-muted">Ultima Actualizacion 11:59 PM</div>
        </div>
    </div>
    <div class="col-lg-4">
        <!-- Example Pie Chart Card-->
        <div class="card mb-3">
            <div class="card-header">
                <i class="fa fa-pie-chart"></i> Gastos Extraordinarios
            </div>
            <div class="card-body">
                <canvas id="myPieChart" width="100%" height="100"></canvas>
            </div>
            <div class="card-footer small text-muted">Ultima Actualizacion 11:59 PM</div>
        </div>
    </div>
</div>
@section js{
    @Scripts.Render("~/Assets/Home")
}